{
  "name": "nomad-netlify-cms-auth",
  "version": "0.0.3",
  "description": "webpacked lambda handlers for netlify cms to authenticate with github",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run tsc:prod && npm run ts node_modules/.bin/webpack",
    "build:prod": "NODE_ENV=production npm run build",
