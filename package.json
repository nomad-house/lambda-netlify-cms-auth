    "ts": "npm run node -- -r ts-node/register",
    "tsc:prod": "node_modules/.bin/tsc --project tsconfig.prod.json",
    "debug": "npm run ts --inspect-brk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomad-house/lambda-netlify-cms-auth.git"
  },
