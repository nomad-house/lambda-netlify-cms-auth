  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@types/aws-lambda": "^8.10.56",
    "@types/node": "^14.0.13",
    "@types/randomstring": "^1.1.6",
    "@types/simple-oauth2": "^2.5.3",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.41.17",
    "babel-loader": "^8.1.0",
